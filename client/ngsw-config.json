{
    "$schema": "../node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "dataGroups":
    [
      {
        "name": "api",
        "urls": ["/api"],
        "cacheConfig": {
          "maxSize": 0,
          "maxAge": "0u",
          "strategy": "freshness"
        }
      }
    ],
    "assetGroups": [{
      "name": "eshop",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/manifest.webmanifest",
          "/*.bundle.css",
          "/*.bundle.js",
          "/*.chunk.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }]
  }
