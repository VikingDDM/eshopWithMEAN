    "stylelint-scss": "^3.17.2",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.1",
    "typescript": "3.7.5",
    "webpack-cli": "^3.3.11"
  }
}
