    "@angular/router": "^12.2.1",
    "@angular/service-worker": "^12.2.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/router-store": "^12.4.0",
    "@ngrx/store": "^12.4.0",
